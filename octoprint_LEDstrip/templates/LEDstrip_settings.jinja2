<h4>LEDstrip Settings</h4>
<form class="form-horizontal">
	<div class="control-group">
		<label class="control-label">{{ _('Red pin') }}</label>
		<div class="controls">
			<input type="text" class="input-block-level" data-bind="value: settings.plugins.LEDstrip.Rpin"/>
		</div>
	</div>
	<div class="control-group">
		<label class="control-label">{{ _('Green pin') }}</label>
		<div class="controls">
			<input type="text" class="input-block-level" data-bind="value: settings.plugins.LEDstrip.Gpin"/>
		</div>
	</div>
	<div class="control-group">
		<label class="control-label">{{ _('Blue pin') }}</label>
		<div class="controls">
			<input type="text" class="input-block-level" data-bind="value: settings.plugins.LEDstrip.Bpin"/>
		</div>
    </div>
</form>
